{
  "name": "commonform-validate",
  "description": "validate Common Forms",
  "version": "0.4.0",
  "author": "Kyle E. Mitchell <kyle@kemitchell.com> (http://kemitchell.com)",
  "bugs": "https://github.com/commonform/commonform-validate/issues",
  "dependencies": {
